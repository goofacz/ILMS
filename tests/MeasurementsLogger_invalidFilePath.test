%file: test.ned
import smile.MeasurementsLogger;

network Test
{
    submodules:
        measurementsLogger: MeasurementsLogger    {
            directoryPath = "!@#";
            fileName = "..#$#*";
            format = "CSV";
        }
}

%inifile: omnet.ini
[General]
cmdenv-express-mode = false
**.cmdenv-log-level = detail
network = Test

%exitcode: 1

%contains: stderr
Error: MeasurementsLogger failed to open !@#/..#$#* 